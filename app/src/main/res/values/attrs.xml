<?xml version="1.0" encoding="utf-8"?>
<resources>
    <item name="navActivityId" type="id" />
    <item name="incrementButtonId" type="id" />
    <item name="decrementButtonId" type="id" />

    <item name="customTextViewDescriptionId" type="id" />
    <item name="customTextViewTitleId" type="id" />

    <item name="garlandStickView" type="id" />
    <item name="garlandTitleTv" type="id" />
    <item name="garlandSettingIv" type="id" />
    <item name="garlandConstraintLayout" type="id" />
    <item name="garlandLinearLayout" type="id" />
    <item name="gralandFrameContainer" type="id" />
    <item name="kaoMojiTv" type="id" />
    <item name="home_navigation_menu" type="id" />
    <item name="list_navigation_menu" type="id" />
    <item name="music_navigation_menu" type="id" />
    <item name="profile_navigation_menu" type="id" />
    <item name="adView" type="id" />

    <declare-styleable name="TitleTextView">
        <attr name="titleName" format="string" />
    </declare-styleable>

    <declare-styleable name="ExpandableTextViewLayout">
        <attr name="maxLines" format="integer" />
    </declare-styleable>

    <declare-styleable name="DynamicHeaderLinearLayout">
        <attr name="headerName" format="string" />
    </declare-styleable>


    <declare-styleable name="CheckBoxHeaderLinearLayout">
        <attr name="checkBoxHeaderName" format="string" />
    </declare-styleable>

    <declare-styleable name="MaxHeightScrollView">
        <attr name="maxHeight" format="dimension" />
    </declare-styleable>


    <declare-styleable name="ImageHeaderLayout">
        <attr name="title" format="string" />
        <attr name="headerImage" format="reference" />
    </declare-styleable>

    <declare-styleable name="DynamicShimmerView">
        <attr name="shimmerLayout" format="reference" />
        <attr name="totalShimmer" format="integer" />
        <attr name="portraitShimmer" format="integer" />
        <attr name="landscapeShimmer" format="integer" />
        <attr name="shimmerOrientation" format="integer" />
        <attr name="shimmerLayoutManager" format="integer" />
    </declare-styleable>

    <declare-styleable name="DynamicTriStatesCheckbox">
        <attr name="mode" format="integer" />
    </declare-styleable>

    <declare-styleable name="IndicatorTextView">
        <attr name="textSize" format="integer" />
        <attr name="textColorType" format="enum">
            <enum name="none" value="0" />
            <enum name="primary" value="1" />
            <enum name="primary_dark" value="2" />
            <enum name="accent" value="3" />
            <enum name="accent_dark" value="4" />
            <enum name="tint_primary" value="5" />
            <enum name="tint_primary_dark" value="6" />
            <enum name="tint_accent" value="7" />
            <enum name="tint_accent_dark" value="8" />
            <enum name="custom" value="9" />
            <enum name="background" value="10" />
            <enum name="tint_background" value="11" />
            <enum name="text_primary" value="12" />
            <enum name="text_secondary" value="13" />
            <enum name="text_primary_inverse" value="14" />
            <enum name="text_secondary_inverse" value="15" />
            <enum name="surface" value="16" />
            <enum name="tint_surface" value="17" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomCheckPreference">
        <attr name="text_size" format="dimension" />
    </declare-styleable>


    <attr name="entries" format="reference" />
    <attr name="icons" format="reference" />

    <declare-styleable name="DynamicIconDslTabLayout">
        <attr name="icons" />
    </declare-styleable>

    <declare-styleable name="DynamicChipDslTabLayout">
        <attr name="entries" />
    </declare-styleable>

    <declare-styleable name="DynamicDslTabLayout">
        <attr name="entries" />
    </declare-styleable>

    <declare-styleable name="AniLibTextTabLayout">
        <attr name="entries" />
    </declare-styleable>

    <declare-styleable name="DynamicPopUpMenu">
        <attr name="entries" />
        <attr name="icons" />
    </declare-styleable>

    <declare-styleable name="CircleSimpleDraweeView">
        <attr name="showBorder" format="boolean" />
        <attr name="colorType" format="boolean" />
    </declare-styleable>


    <declare-styleable name="CustomDynamicHeader">
        <attr name="titleAlignCenter" format="boolean" />
        <attr name="subtitleAlignCenter" format="boolean" />
        <attr name="titleTextSize" format="dimension" />
        <attr name="subtitleTextSize" format="dimension" />
        <attr name="copySubtitleOnLongClick" format="boolean" />
    </declare-styleable>

    <declare-styleable name="HorizontalHeaderLayout">
        <attr name="titleText" format="string" />
        <attr name="subtitleText" format="string" />
    </declare-styleable>

    <declare-styleable name="AniLibSpinner">
        <attr name="add_stroke" format="boolean" />
    </declare-styleable>
</resources>