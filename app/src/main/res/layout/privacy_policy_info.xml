<?xml version="1.0" encoding="utf-8"?>
<com.pranavpandey.android.dynamic.support.widget.DynamicLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <com.pranavpandey.android.dynamic.support.widget.DynamicCardView style="@style/Widget.DynamicApp.Card">

        <com.pranavpandey.android.dynamic.support.view.DynamicHeader
            android:id="@+id/privacyPolicy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ads_icon="@drawable/ic_privacy_policy"
            app:ads_subtitle="@string/privacy_policy_desc"
            app:ads_title="@string/privacy_policy" />

    </com.pranavpandey.android.dynamic.support.widget.DynamicCardView>

    <com.pranavpandey.android.dynamic.support.widget.DynamicCardView style="@style/Widget.DynamicApp.Card">

        <com.pranavpandey.android.dynamic.support.view.DynamicHeader
            android:id="@+id/termsAndCondition"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ads_icon="@drawable/ads_ic_license"
            app:ads_subtitle="@string/terms_and_condition_desc"
            app:ads_title="@string/terms_and_condition" />

    </com.pranavpandey.android.dynamic.support.widget.DynamicCardView>


    <com.pranavpandey.android.dynamic.support.widget.DynamicCardView style="@style/Widget.DynamicApp.Card">

        <com.pranavpandey.android.dynamic.support.setting.DynamicCheckPreference
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ads_checked="true"
            app:ads_key="crashReportKey"
            app:ads_icon="@drawable/ic_bug_report"
            app:ads_title="@string/crash_reporting"
            app:ads_description="@string/crash_report_desc"/>

    </com.pranavpandey.android.dynamic.support.widget.DynamicCardView>

</com.pranavpandey.android.dynamic.support.widget.DynamicLinearLayout>